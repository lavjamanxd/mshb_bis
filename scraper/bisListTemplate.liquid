msh_bis_addon_data = {
    ["version"] = "{{ version }}",
    ["phases"] = {
        {%- for phase in phases %}
        ["phase{{ phase[0] }}"]= {
        {%- for classGroup in phase[1] %}
            ["{{ classGroup[0]  | downcase }}"] = {
            {%- for spec in classGroup[1].specs %}
                {
                    ["spec"] = "{{ spec.spec | downcase }}",
                    ["role"] = "{{ spec.role }}",
                    ["items"] = {
                    {%- for itemslot in spec.items %}
                        ["{{ itemslot[0] }}"] = {
                            {%- for itemGroup in itemslot[1] -%}
                                {%- for itemId in itemGroup -%}
                                    "{{ itemId }}",
                                {%- endfor -%}
                            {%- endfor -%}
                        },
                    {%- endfor -%}
                    }
                },
               {%- endfor %}
            },
        {%- endfor %}
        },
        {%- endfor %}
    }
}