metadata = {
    {%- for item in metadata %}
        ["{{ item[0] }}"] = {
            ["source"] = {
                {%- if item[1].source.profession %}
                ["profession"] = {
                    ["name"] = "{{ item[1].source.profession.name }}",
                },
                {%- endif %}
                {%- if item[1].source.quest %}
                ["quest"] = {
                    ["name"] = "{{ item[1].source.quest.name }}",
                    ["zone"] = "{{ item[1].source.quest.zone }}",
                },
                {%- endif %}
                {%- if item[1].source.drop %}
                ["drop"] = {
                    ["name"] = "{{ item[1].source.drop.name }}",
                    ["zone"] = "{{ item[1].source.drop.zone }}",
                    ["chance"] = "{{ item[1].source.drop.chance }}",
                },
                {%- endif %}
                {%- if item[1].source.containedin %}
                ["containedin"] = {
                    ["name"] = "{{ item[1].source.containedin.name }}",
                    ["zone"] = "{{ item[1].source.containedin.zone }}",
                    ["chance"] = "{{ item[1].source.containedin.chance }}",
                },
                {%- endif %}
                {%- if item[1].source.soldBy %}
                ["soldby"] = {
                    ["name"] = "{{ item[1].source.soldBy.name }}",
                    ["zone"] = "{{ item[1].source.soldBy.zone }}",
                    ["price"] = {
                    {%- if item[1].source.soldBy.price.money %}
                        ["money"] = { {{ item[1].source.soldBy.price.money.gold }}, {{ item[1].source.soldBy.price.money.silver }}, {{ vendor.price.money.copper }} },
                    {%- endif %}
                    {%- if item[1].source.soldBy.price.honor %}
                        ["honor"] = {{ item[1].source.soldBy.price.honor }},
                    {%- endif %}
                    {%- if item[1].source.soldBy.price.arena %}
                        ["arena"] = {{ item[1].source.soldBy.price.arena }},
                    {%- endif %}
                    {%- if item[1].source.soldBy.price.item %}
                        ["item"] = {
                        {%- for item in item[1].source.soldBy.price.item  %}
                            {
                                ["id"] = {{ item.id }},
                                ["amount"] = {{ item.amount }}
                            },
                        {%- endfor %}
                        },
                    {%- endif %}
                    },
                },
                {%- endif %}
            }
        },
    {%- endfor %}
}